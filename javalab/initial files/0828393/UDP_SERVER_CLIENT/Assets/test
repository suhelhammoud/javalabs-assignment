<script type="text/javascript">
function yschblonToggle(oChkbx) {
 var oCDiv = oChkbx.parentNode;
 if (oCDiv) {
 var aLabels = oCDiv.getElementsByTagName("label");
 var aRadioBtns = oCDiv.getElementsByTagName("input");
 var sClassName = oChkbx.checked ? 'disabled': '';
 var aLabelsLen = aLabels.length - 1;
 if (aLabels) {
 for (var i = 0; i < aLabelsLen; i++) {
 aLabels[i].className = sClassName;
 }
 }
 if (aRadioBtns) {
 if (sClassName == 'disabled') {
 aRadioBtns[0].checked = true;
 } else {
 aRadioBtns[1].checked = true;
 }
 for (var j = 0; j < aLabelsLen; j++) {
 aRadioBtns[j].disabled = oChkbx.checked;
 }
 }
 }
}
</script>
