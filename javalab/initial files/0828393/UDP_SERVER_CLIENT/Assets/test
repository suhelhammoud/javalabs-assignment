  YAHOO.Fp.oSearch.searchMore();
 
 
 if (document.getElementById("sizetogglelink")) {
 document.getElementById("sizetogglelink").onclick = YAHOO.Fp.setFPS;
 }
 
 YAHOO.Fp.windowshadeAnimation = function() {
 // slide out on click
 var wShade = document.getElementById('window-shade');
 var wShadeStayBtn = YAHOO.util.Dom.getElementsByClassName('cta', 'a', wShade);
 var wShadeFeedbackLinks = document.getElementById('wsfeedback').getElementsByTagName('a');
 var wShadeAnimAttr = { 'height' : { 'to': 0 } };
 var wsSlideOut = new YAHOO.util.Anim(wShade, wShadeAnimAttr, 1, YAHOO.util.Easing.easeBoth);
 
 var showEyebrow = function() {
 var eyebrow = document.getElementById('eyebrow');
 var ebSlideInAttr = {'height' : { 'to' : 14 }};
 var ebSlideIn = new YAHOO.util.Anim(eyebrow, ebSlideInAttr, 0.5, YAHOO.util.Easing.easeIn);
 ebSlideIn.animate();
 };
 wsSlideOut.onComplete.subscribe(showEyebrow); 
 YAHOO.util.Event.addListener(wShadeStayBtn, 'click', function(e) {
 wsSlideOut.animate();
 // todo write in JS ULT Magic here!
 //var img = new Image();
 //img.src = 'someultlink';
 YAHOO.util.Event.stopEvent(e);
 });
 YAHOO.util.Event.addListener(wShadeFeedbackLinks, 'click', function(e) {
 wShadeFeedbackWin = window.open(this.href,'wsFeedbackWin','width=980,height=600,menubar=yes,resizable=yes,scrollbars=yes,location=yes,');
 if (window.focus) { wShadeFeedbackWin.focus(); }
 YAHOO.util.Event.stopEvent(e);
 });
 //slide in windowshade
 var wShadeAnimInAttr = {
 'height': { 'from': 0, 'to': 4, unit: 'em' }
 };
 var wsSlideIn = new YAHOO.util.Anim(wShade, wShadeAnimInAttr, 1, YAHOO.util.Easing.easeBoth);
 wsSlideIn.animate();
 
};
YAHOO.util.Event.onDOMReady(YAHOO.Fp.windowshadeAnimation);
}
